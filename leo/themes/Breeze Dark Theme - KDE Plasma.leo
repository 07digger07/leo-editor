<t tx="ekr.20180307031524.122">QMenu {
  color: @text-foreground-bright
}
QMenuBar::item:selected {
  color: @dark-base0;
  background: @dark-base03;
  border: 2px solid @dark-base03;
  border-bottom-color: @solarized-blue; /* same as the pane color */
  padding: 2px;
}
QMenu::item {
  color: @dark-base0;
  background: #4a5159;
  border: 8px solid #4a5159;
}
QMenu::item:selected {
  color: @dark-base0;
  background: @dark-base01;
  border: 2px solid @dark-base01;
  border-bottom-color: #2eacff; /* same as the pane color */
  padding: 2px;
}
QMenuBar::item {
  color: @text-foreground;
  background: @dark-base03;
}
QMenuBar::item:pressed {
  color: @text-foreground-bright;
  background: @dark-base01;
}
</t>
<t tx="ekr.20180307031524.123">/* The mini-buffer */
QLabel#minibufferLabel {
