<t tx="ekr.20180307031524.127">/* The status area */
QLineEdit#status1 {
  background: @dark-base03;
  border-color: @dark-base02;
  border-width: 1px;
  border-style: solid;
}
QLineEdit#status2 {
  background: @dark-base03;
  border-color: @dark-base02;
  border-width: 1px;
  border-style: solid;
}
QLineEdit#status1[style_class ~= 'info'],
QLineEdit#status2[style_class ~= 'info'] {
    background: @solarized-blue;
    color:      @solarized-white;
}

QLineEdit#status1[style_class ~= 'fail'],
QLineEdit#status2[style_class ~= 'fail'] {
    background: @solarized-red;
    color:      @solarized-white;
}
</t>
<t tx="ekr.20180307031524.128">QTabWidget::pane { /* The tab widget frame */
    border-top: 2px solid #31363b;
}

QTabWidget::tab-bar {
    left: 5px; /* move to the right by 5px */
}

/* Style the tab using the tab sub-control. */
QTabBar::tab {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #232629, stop: 0.4 #23262A,
                                stop: 0.5 #23262A, stop: 1.0 #232629);
    border: 4px solid #31363b;
    border-bottom-color: #31363b; /* same as the pane color */
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
    min-width: 8ex;
    padding: 2px;
}

QTabBar::tab:selected, QTabBar::tab:hover {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #23262A, stop: 0.4 #232629,
                                stop: 0.5 #232629, stop: 1.0 #23262A);
    border-bottom-color: #1a6394;
}

QTabBar::tab:selected {
    border-color: #31363b;
    border-bottom-color: @solarized-blue; /* same as pane color */
}

QTabBar::tab:!selected {
    margin-top: 2px; /* make non-selected tabs look smaller */
}</t>
<t tx="ekr.20180307031524.129">LeoQTreeWidget#treeWidget {
  /* These apply to the selected item, but not to editing items.*/
  background: @dark-base03;
  color: @dark-base0;
  show-decoration-selected: 1; /* Extend the selection */
  padding: 2px solid @dark-base01;
  border-color: @dark-base03;
  font-family: @font-family-tree;
  font-size: @font-size-tree;
  font-weight: normal; /* normal,bold,100,..,900 */
  font-style: normal; /* normal, italic,oblique */
}
QTreeView::item:selected, QTreeView::item:active:selected {
  background: @dark-base01;
}
QTreeView::item:active:selected {
  background: @dark-base01;
  color: @dark-base0;
}
QTreeView::branch:closed:has-children{
    image: @tree-image-closed;
}
QTreeView::branch:open:has-children{
    image: @tree-image-open;
}

/* Headline edit widgets */
QTreeView QLineEdit {
  color: @dark-base03;
  background: @solarized-violet;
  selection-color: @solarized-violet; 
  selection-background-color: @dark-base3;
  font-family: @font-family-tree;
  font-size: @font-size-tree;
  font-weight: normal; /* normal,bold,100,..,900 */
  font-style: normal; /* normal, italic,oblique */
  padding: 0px;
}

QTreeWidget {
  /* These apply to the selected item, but not to editing items.*/
  background: @dark-base01;
  font-family: @font-family-tree;
  font-size: @font-size-tree;
  font-weight: normal; /* normal,bold,100,..,900 */
  font-style: normal; /* normal, italic,oblique */
  padding: 2px solid @dark-base03;
  border-color: @dark-base01;
}
QTreeWidget::selected {
  background: @dark-base01;
  color: @dark-base03; 
}
</t>
