<t tx="chris.20180308171552.125">QMenuBar
{
    background-color: @dark-base3;
    color: @dark-base0;
}

QMenuBar::item
{
    background: transparent;
}

QMenuBar::item:selected
{
    background: @dark-base7;
    border: 0.1ex solid @dark-base4;
}

QMenuBar::item:pressed
{
    border: 0.1ex solid @dark-base4;
    background-color: @dark-base6;
    color: @dark-base0;
    margin-bottom: -0.1ex;
    padding-bottom: 0.1ex;
}

QMenu
{
    border: 0.1ex solid @dark-base4;
    color: @dark-base0;
    margin: 0.2ex;
}

QMenu::icon
{
    margin: 0.5ex;
}

QMenu::item
{
    padding: 0.5ex 3ex 0.5ex 3ex;
    margin-left: 0.5ex;
    border: 0.1ex solid transparent; /* reserve space for selection border */
}

QMenu::item:selected
{
    background: @dark-base7;
    color: @dark-base0;
}

QMenu::separator
{
    height: 0.2ex;
    background: lightblue;
    margin-left: 1ex;
    margin-right: 0.5ex;
}</t>
