<t tx="chris.20180311131013.1">/* ===== From qt-gui-plugin-style-sheet ===== */

/*
@language css
at-tabwidth -2 causes a crash in parse-template!
*/
QDockWidget {
border: 5px solid red;
}
QDoubleSpinBox, QSpinBox, QDateEdit, QTimeEdit {
border: none;
padding: 3px;
margin-right: 5px;
}
QLabel {
color: #575b5f;
}
QLineEdit {
border: none;
padding: 3px;
}
QWidget {
/* Not for text widgets */
background:  #31363b;
color: #eff0f1;
border-color: #575b5f;
font-size: 16px;
}
/* The body pane */
QTextEdit#richTextEdit {
background-color: #232629;
color: #eff0f1;
border: 0.1ex solid #575b5f;
font-family: Inconsolata,Droid Sans Mono, DejaVu Sans Mono;
font-size: 22px;
font-weight: normal; /* normal,bold,100,..,900 */
font-style: normal; /* normal,italic,oblique */
padding-left: 3px;
padding-top: 3px;
padding-bottom: 3px;
padding-right: 3px;
height: 200%;
}
/* active pane highlighting */
QTextEdit#log-widget, LeoQTreeWidget#treeWidget, QTextEdit#richTextEdit {
border-style: solid;
border-width: 3px;
}
QTextEdit#log-widget {
border-color: #31363b;
}
LeoQTreeWidget#treeWidget {
border-color: #31363b;
}
QTextEdit#richTextEdit {
border-color: #31363b;
}
QTextEdit:focus#log-widget,LeoQTreeWidget:focus#treeWidget, QTextEdit:focus#richTextEdit {
border-color: #268bd2;
}
QPushButton
{
color: #eff0f1;
background-color: qlineargradient(x1: 0.5, y1: 0.5 x2: 0.5, y2: 1, stop: 0 #575b5f, stop: 0.5 #31363b);
border-width: 0.1ex;
border-color: #575b5f;
border-style: solid;
padding: 0.5ex;
border-radius: 0.2ex;
outline: none;
}
QPushButton:disabled
{
background-color: #31363b;
border-width: 0.1ex;
border-color: #575b5f;
border-style: solid;
padding-top: 0.5ex;
padding-bottom: 0.5ex;
padding-left: 1ex;
padding-right: 1ex;
border-radius: 0.2ex;
color: #575b5f;
}
QPushButton:hover
{
background-color: #224e65;
}
QPushButton:focus
{
color: white;
}
QPushButton:pressed
{
background-color: #31363b;
padding-top: -1.5ex;
padding-bottom: -1.7ex;
}
QTextEdit#log-widget {
background-color: #31363b;
color: #eff0f1;
/* selection-color: white; */
/* selection-background-color: blue; */
font-family: Inconsolata,Droid Sans Mono, DejaVu Sans Mono;
font-size: 16px;
font-weight: normal; /* normal,bold,100,..,900 */
font-style: normal; /* normal, italic,oblique */
}
QLineEdit#findPattern {
background-color: #232629;
}
QLineEdit#findChange {
background-color: #232629;
}
QListWidget#nodetags-listWidget {
background-color: #232629;
}
QMenuBar
{
background-color: #31363b;
color: #eff0f1;
}
QMenuBar::item
{
background: transparent;
}
QMenuBar::item:selected
{
background: #224e65;
border: 0.1ex solid #575b5f;
}
QMenuBar::item:pressed
{
border: 0.1ex solid #575b5f;
background-color: #3daee9;
color: #eff0f1;
margin-bottom: -0.1ex;
padding-bottom: 0.1ex;
}
QMenu
{
