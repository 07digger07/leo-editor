<t tx="ekr.20120313134250.14357">def isSectionDef (self,p):
    
    return self.isSectionRef(p.h)
    
def isSectionRef (self,s):

    n1 = s.find("&lt;&lt;",0)
    n2 = s.find("&gt;&gt;",0)
    return -1 &lt; n1 &lt; n2 and s[n1+2:n2].strip()
